<p id="notice"><%= notice %></p>

<h1>Tipo Solicitudes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre tipo</th>
      <th>Id parametros</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_solicitudes.each do |tipo_solicitude| %>
      <tr>
        <td><%= tipo_solicitude.nombre_tipo %></td>
        <td><%= tipo_solicitude.id_parametros %></td>
        <td><%= link_to 'Show', tipo_solicitude %></td>
        <td><%= link_to 'Edit', edit_tipo_solicitude_path(tipo_solicitude) %></td>
        <td><%= link_to 'Destroy', tipo_solicitude, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Solicitude', new_tipo_solicitude_path %>
