<p id="notice"><%= notice %></p>

<h1>Solicituds</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Fecha solicitud</th>
      <th>Hora</th>
      <th>Idtipo solicitud</th>
      <th>Id usuario</th>
      <th>Id documento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicituds.each do |solicitud| %>
      <tr>
        <td><%= solicitud.descripcion %></td>
        <td><%= solicitud.fecha_solicitud %></td>
        <td><%= solicitud.hora %></td>
        <td><%= solicitud.idtipo_solicitud %></td>
        <td><%= solicitud.id_usuario %></td>
        <td><%= solicitud.id_documento %></td>
        <td><%= link_to 'Show', solicitud %></td>
        <td><%= link_to 'Edit', edit_solicitud_path(solicitud) %></td>
        <td><%= link_to 'Destroy', solicitud, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitud', new_solicitud_path %>
