<p id="notice"><%= notice %></p>

<h1>Tipo Seguimientos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_seguimientos.each do |tipo_seguimiento| %>
      <tr>
        <td><%= tipo_seguimiento.nombre %></td>
        <td><%= link_to 'Show', tipo_seguimiento %></td>
        <td><%= link_to 'Edit', edit_tipo_seguimiento_path(tipo_seguimiento) %></td>
        <td><%= link_to 'Destroy', tipo_seguimiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Seguimiento', new_tipo_seguimiento_path %>
